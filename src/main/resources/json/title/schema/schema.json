{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string"
            },
            "id": {
                "type": "number"
            },
            "sex": {
                "type": "string",
                "enum": [
                    "male",
                    "female"
                ]
            }
        },
        "required": [
            "id"
        ]
    }
}